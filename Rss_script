
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "RSS | God Mode Menu",
   LoadingTitle = "–ó–∞–≥—Ä—É–∑–∫–∞ —Å–∏—Å—Ç–µ–º—ã –ø—Ä–∏—Ç—è–∂–µ–Ω–∏—è...",
   LoadingSubtitle = "by Claude AI for Delta",
   ConfigurationSaving = { Enabled = false }
})

local Tab = Window:CreateTab("‚öΩ –§—É–Ω–∫—Ü–∏–∏ –ú—è—á–∞", 4483362458)
local TabGoalkeeper = Window:CreateTab("ü•Ö –í—Ä–∞—Ç–∞—Ä—å", 4483362458)

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local Settings = {
    MagnetEnabled = false,
    Method = "–ü–ª–∞–≤–Ω–æ–µ –ø—Ä–∏—Ç—è–∂–µ–Ω–∏–µ",
    Range = 25,
    Strength = 50,
    ShowRadius = false,
    AutoSave = false,
    SaveDistance = 20,
    SaveSpeed = 120,
    AutoMove = true
}

-- –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è —Ä–∞–¥–∏—É—Å–∞
local radiusPart = nil
local goalkeeperZone = nil

local function createRadiusVisual()
    if radiusPart then radiusPart:Destroy() end
    
    radiusPart = Instance.new("Part")
    radiusPart.Name = "MagnetRadius"
    radiusPart.Anchored = true
    radiusPart.CanCollide = false
    radiusPart.Shape = Enum.PartType.Ball
    radiusPart.Material = Enum.Material.ForceField
    radiusPart.Color = Color3.fromRGB(0, 255, 255)
    radiusPart.Transparency = 0.7
    radiusPart.Size = Vector3.new(Settings.Range * 2, Settings.Range * 2, Settings.Range * 2)
    radiusPart.Parent = workspace
    
    local highlight = Instance.new("SelectionBox")
    highlight.LineThickness = 0.05
    highlight.Color3 = Color3.fromRGB(0, 255, 255)
    highlight.SurfaceColor3 = Color3.fromRGB(0, 255, 255)
    highlight.SurfaceTransparency = 0.9
    highlight.Adornee = radiusPart
    highlight.Parent = radiusPart
    
    return radiusPart
end

local function updateRadiusVisual()
    if Settings.ShowRadius and radiusPart then
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            radiusPart.Position = char.HumanoidRootPart.Position
            radiusPart.Size = Vector3.new(Settings.Range * 2, Settings.Range * 2, Settings.Range * 2)
        end
    end
end

-- –£–õ–£–ß–®–ï–ù–ù–´–ô –ü–û–ò–°–ö –ú–Ø–ß–ê (–¥–ª—è –∏–≥—Ä—ã, –Ω–µ –ª–æ–±–±–∏!)
local function getBall()
    -- –°–Ω–∞—á–∞–ª–∞ –∏—â–µ–º –≤ –∞–∫—Ç–∏–≤–Ω—ã—Ö –∑–æ–Ω–∞—Ö –∏–≥—Ä—ã
    local awayPosition = workspace:FindFirstChild("AwayPosition")
    local homePosition = workspace:FindFirstChild("HomePosition")
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –º—è—á–∏ –≤ –∏–≥—Ä–æ–≤—ã—Ö –∑–æ–Ω–∞—Ö
    if awayPosition or homePosition then
        for _, v in pairs(workspace:GetDescendants()) do
            if v.Name == "SoccerBall" and v:IsA("BasePart") then
                -- –ò—Å–∫–ª—é—á–∞–µ–º –º—è—á–∏ –∏–∑ –ª–æ–±–±–∏
                local fullPath = v:GetFullName()
                if not fullPath:find("Lobby") and not fullPath:find("SoccerBalls") then
                    -- –≠—Ç–æ –∏–≥—Ä–æ–≤–æ–π –º—è—á!
                    return v
                end
            end
        end
    end
    
    -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫: –∏—â–µ–º –º—è—á –∫–æ—Ç–æ—Ä—ã–π –¥–≤–∏–∂–µ—Ç—Å—è
    for _, v in pairs(workspace:GetDescendants()) do
        if v.Name == "SoccerBall" and v:IsA("BasePart") then
            if v.AssemblyLinearVelocity.Magnitude > 0.1 or v.Parent.Name ~= "SoccerBalls" then
                return v
            end
        end
    end
    
    -- –ü–æ—Å–ª–µ–¥–Ω–∏–π –≤–∞—Ä–∏–∞–Ω—Ç: –ª—é–±–æ–π –º—è—á –Ω–µ –≤ –ª–æ–±–±–∏
    for _, v in pairs(workspace:GetDescendants()) do
        if v.Name == "SoccerBall" and v:IsA("BasePart") then
            if not v:GetFullName():find("Lobby") then
                return v
            end
        end
    end
    
    return nil
end

-- –ü–û–ò–°–ö –í–û–†–û–¢ (—É–ª—É—á—à–µ–Ω–Ω—ã–π)
local function findGoal()
    -- –ò—â–µ–º GolieCenter (—Ü–µ–Ω—Ç—Ä –≤–æ—Ä–æ—Ç)
    local awayGoal = workspace:FindFirstChild("AwayPosition")
    local homeGoal = workspace:FindFirstChild("HomePosition")
    
    if awayGoal then
        local goalieCenter = awayGoal:FindFirstChild("GoalieCenter")
        if goalieCenter then return goalieCenter end
    end
    
    if homeGoal then
        local goalieCenter = homeGoal:FindFirstChild("GoalieCenter")
        if goalieCenter then return goalieCenter end
    end
    
    -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫
    for _, v in pairs(workspace:GetDescendants()) do
        if v.Name == "GoalieCenter" and v:IsA("BasePart") then
            return v
        end
    end
    
    return nil
end

-- –û–ø—Ä–µ–¥–µ–ª—è–µ–º, –≤ –∫–∞–∫–∏—Ö –≤–æ—Ä–æ—Ç–∞—Ö –º—ã —Å—Ç–æ–∏–º
local function getMyGoal()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return nil end
    
    local hrp = char.HumanoidRootPart
    
    -- –ò—â–µ–º –±–ª–∏–∂–∞–π—à–∏–π GoalieCenter
    local awayGoal = workspace:FindFirstChild("AwayPosition")
    local homeGoal = workspace:FindFirstChild("HomePosition")
    
    local closestGoal = nil
    local closestDist = math.huge
    
    if awayGoal and awayGoal:FindFirstChild("GoalieCenter") then
        local dist = (hrp.Position - awayGoal.GoalieCenter.Position).Magnitude
        if dist < closestDist then
            closestDist = dist
            closestGoal = awayGoal.GoalieCenter
        end
    end
    
    if homeGoal and homeGoal:FindFirstChild("GoalieCenter") then
        local dist = (hrp.Position - homeGoal.GoalieCenter.Position).Magnitude
        if dist < closestDist then
            closestDist = dist
            closestGoal = homeGoal.GoalieCenter
        end
    end
    
    return closestGoal
end

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª
local RunService = game:GetService("RunService")
local connection

local function startMagnet()
    if connection then connection:Disconnect() end
    
    connection = RunService.Heartbeat:Connect(function()
        -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Ä–∞–¥–∏—É—Å–∞
        if Settings.ShowRadius then
            updateRadiusVisual()
        end
        
        local player = game.Players.LocalPlayer
        local char = player.Character
        if not char then return end
        
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        
        local ball = getBall()
        if not ball then return end
        
        -- === –†–ï–ñ–ò–ú –í–†–ê–¢–ê–†–Ø (–£–õ–£–ß–®–ï–ù–ù–´–ô) ===
        if Settings.AutoSave then
            local myGoal = getMyGoal()
            
            if myGoal then
                -- –û–±–Ω–æ–≤–ª—è–µ–º –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—é –∑–æ–Ω—ã
                if goalkeeperZone then
                    goalkeeperZone.Position = myGoal.Position
                    goalkeeperZone.Size = Vector3.new(Settings.SaveDistance * 2, Settings.SaveDistance * 2, Settings.SaveDistance * 2)
                end
                
                local distBallToGoal = (myGoal.Position - ball.Position).Magnitude
                local distPlayerToGoal = (myGoal.Position - hrp.Position).Magnitude
                
                -- –ï—Å–ª–∏ –º—è—á –ø—Ä–∏–±–ª–∏–∂–∞–µ—Ç—Å—è –∫ –≤–æ—Ä–æ—Ç–∞–º
                if distBallToGoal <= Settings.SaveDistance then
                    local ballVelocity = ball.AssemblyLinearVelocity
                    
                    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –º—è—á–∞ (–ª–µ—Ç–∏—Ç –ª–∏ –∫ –≤–æ—Ä–æ—Ç–∞–º?)
                    local directionToGoal = (myGoal.Position - ball.Position).Unit
                    local ballDirection = ballVelocity.Unit
                    local dotProduct = directionToGoal:Dot(ballDirection)
                    
                    -- –ï—Å–ª–∏ –º—è—á –ª–µ—Ç–∏—Ç –ö –≤–æ—Ä–æ—Ç–∞–º (dotProduct > 0) –∏ –±—ã—Å—Ç—Ä–æ
                    if ballVelocity.Magnitude > 5 and dotProduct > 0.3 then
                        -- –ü–æ–ª—É—á–∞–µ–º –≤–ª–∞–¥–µ–Ω–∏–µ –º—è—á–æ–º
                        pcall(function()
                            ball.AssemblyLinearVelocity = ball.AssemblyLinearVelocity
                        end)
                        
                        -- –û–¢–ë–ò–í–ê–ï–ú –ú–Ø–ß –æ—Ç –≤–æ—Ä–æ—Ç
                        local saveDirection = (ball.Position - myGoal.Position).Unit
                        saveDirection = Vector3.new(saveDirection.X, 0.3, saveDirection.Z).Unit -- –ù–µ–º–Ω–æ–≥–æ –≤–≤–µ—Ä—Ö
                        
                        ball.AssemblyLinearVelocity = saveDirection * Settings.SaveSpeed
                        
                        -- –î–≤–∏–≥–∞–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –∫ –º—è—á—É (–µ—Å–ª–∏ –≤–∫–ª—é—á–µ–Ω–æ)
                        if Settings.AutoMove and distPlayerToGoal < Settings.SaveDistance then
                            local moveTo = ball.Position
                            hrp.CFrame = CFrame.new(
                                hrp.Position:Lerp(moveTo, 0.4),
                                moveTo
                            )
                        end
                        
                        -- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
                        if math.random(1, 10) == 1 then -- –ù–µ —Å–ø–∞–º–∏–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è–º–∏
                            Rayfield:Notify({
                                Title = "ü•Ö –°–ï–ô–í!", 
                                Content = "–ú—è—á –æ—Ç–±–∏—Ç!", 
                                Duration = 1
                            })
                        end
                    end
                end
            end
        end
        
        -- === –ú–ê–ì–ù–ò–¢ –ú–Ø–ß–ê ===
        if Settings.MagnetEnabled then
            local distance = (hrp.Position - ball.Position).Magnitude
            
            if distance <= Settings.Range then
                pcall(function()
                    ball.AssemblyLinearVelocity = ball.AssemblyLinearVelocity
                end)
                
                if Settings.Method == "–§–∏–∑–∏—á–µ—Å–∫–∞—è —Å–∏–ª–∞" then
                    local direction = (hrp.Position - ball.Position).Unit
                    local speed = math.clamp(distance * 2, 20, Settings.Strength)
                    ball.AssemblyLinearVelocity = direction * speed
                
                elseif Settings.Method == "–ñ–µ—Å—Ç–∫–æ–µ –ø—Ä–∏–ª–∏–ø–∞–Ω–∏–µ" then
                    ball.CFrame = hrp.CFrame * CFrame.new(0, -2.5, -3)
                    ball.AssemblyLinearVelocity = hrp.AssemblyLinearVelocity
                    ball.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
                
                elseif Settings.Method == "–í–µ–∫—Ç–æ—Ä–Ω—ã–π –∑–∞—Ö–≤–∞—Ç" then
                    if not ball:FindFirstChild("BV_Magnet") then
                        local bv = Instance.new("BodyVelocity")
                        bv.Name = "BV_Magnet"
                        bv.MaxForce = Vector3.new(5e4, 5e4, 5e4)
                        bv.Parent = ball
                    end
                    
                    local bv = ball:FindFirstChild("BV_Magnet")
                    if bv then
                        bv.Velocity = (hrp.Position - ball.Position).Unit * 70
                    end
                
                elseif Settings.Method == "–ü–ª–∞–≤–Ω–æ–µ –ø—Ä–∏—Ç—è–∂–µ–Ω–∏–µ" then
                    local direction = (hrp.Position - ball.Position).Unit
                    local currentVel = ball.AssemblyLinearVelocity
                    local targetVel = direction * Settings.Strength
                    ball.AssemblyLinearVelocity = currentVel:Lerp(targetVel, 0.3)
                end
            else
                if ball:FindFirstChild("BV_Magnet") then
                    ball.BV_Magnet:Destroy()
                end
            end
        end
    end)
end

startMagnet()

-- === –í–ö–õ–ê–î–ö–ê: –§–£–ù–ö–¶–ò–ò –ú–Ø–ß–ê ===
Tab:CreateToggle({
   Name = "üß≤ –í–∫–ª—é—á–∏—Ç—å –ú–∞–≥–Ω–∏—Ç",
   CurrentValue = false,
   Callback = function(Value) 
       Settings.MagnetEnabled = Value 
       if Value then
           Rayfield:Notify({
               Title = "‚úÖ –ú–∞–≥–Ω–∏—Ç –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω", 
               Content = "–†–∞–¥–∏—É—Å: " .. Settings.Range .. "–º", 
               Duration = 3
           })
       else
           local ball = getBall()
           if ball and ball:FindFirstChild("BV_Magnet") then
               ball.BV_Magnet:Destroy()
           end
       end
   end,
})

Tab:CreateToggle({
   Name = "üëÅÔ∏è –ü–æ–∫–∞–∑–∞—Ç—å —Ä–∞–¥–∏—É—Å –¥–µ–π—Å—Ç–≤–∏—è",
   CurrentValue = false,
   Callback = function(Value) 
       Settings.ShowRadius = Value 
       if Value then
           createRadiusVisual()
       else
           if radiusPart then 
               radiusPart:Destroy() 
               radiusPart = nil
           end
       end
   end,
})

Tab:CreateDropdown({
   Name = "‚öôÔ∏è –ú–µ—Ç–æ–¥ –ø—Ä–∏—Ç—è–∂–µ–Ω–∏—è",
   Options = {"–§–∏–∑–∏—á–µ—Å–∫–∞—è —Å–∏–ª–∞", "–ñ–µ—Å—Ç–∫–æ–µ –ø—Ä–∏–ª–∏–ø–∞–Ω–∏–µ", "–í–µ–∫—Ç–æ—Ä–Ω—ã–π –∑–∞—Ö–≤–∞—Ç", "–ü–ª–∞–≤–Ω–æ–µ –ø—Ä–∏—Ç—è–∂–µ–Ω–∏–µ"},
   CurrentValue = "–ü–ª–∞–≤–Ω–æ–µ –ø—Ä–∏—Ç—è–∂–µ–Ω–∏–µ",
   Callback = function(Option) 
       Settings.Method = Option 
       local ball = getBall()
       if ball and ball:FindFirstChild("BV_Magnet") then
           ball.BV_Magnet:Destroy()
       end
   end,
})

Tab:CreateSlider({
   Name = "üìè –î–∏—Å—Ç–∞–Ω—Ü–∏—è –∑–∞—Ö–≤–∞—Ç–∞",
   Range = {10, 100},
   Increment = 5,
   CurrentValue = 25,
   Callback = function(Value) 
       Settings.Range = Value 
       if radiusPart then
           radiusPart.Size = Vector3.new(Value * 2, Value * 2, Value * 2)
       end
   end,
})

Tab:CreateSlider({
   Name = "üí™ –°–∏–ª–∞ –º–∞–≥–Ω–∏—Ç–∞",
   Range = {10, 150},
   Increment = 10,
   CurrentValue = 50,
   Callback = function(Value) Settings.Strength = Value end,
})

Tab:CreateSection("üîß –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏")

Tab:CreateButton({
   Name = "üîç –ù–∞–π—Ç–∏ –º—è—á (–¥–µ–±–∞–≥)",
   Callback = function()
       local ball = getBall()
       if ball then
           local char = game.Players.LocalPlayer.Character
           if char and char:FindFirstChild("HumanoidRootPart") then
               local dist = (char.HumanoidRootPart.Position - ball.Position).Magnitude
               Rayfield:Notify({
                   Title = "‚úÖ –ò–≥—Ä–æ–≤–æ–π –º—è—á –Ω–∞–π–¥–µ–Ω!", 
                   Content = "–î–∏—Å—Ç–∞–Ω—Ü–∏—è: " .. math.floor(dist) .. "–º | " .. ball:GetFullName(), 
                   Duration = 6
               })
           end
       else
           Rayfield:Notify({
               Title = "‚ùå –ú—è—á –Ω–µ –Ω–∞–π–¥–µ–Ω", 
               Content = "–ó–∞–π–¥–∏—Ç–µ –≤ –º–∞—Ç—á!", 
               Duration = 4
           })
       end
   end,
})

Tab:CreateButton({
   Name = "üìç –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –º—è—á—É",
   Callback = function()
       local ball = getBall()
       local char = game.Players.LocalPlayer.Character
       if ball and char and char:FindFirstChild("HumanoidRootPart") then
           char.HumanoidRootPart.CFrame = ball.CFrame * CFrame.new(0, 5, 0)
           Rayfield:Notify({Title = "‚úÖ –¢–µ–ª–µ–ø–æ—Ä—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω", Content = "", Duration = 2})
       end
   end,
})

-- === –í–ö–õ–ê–î–ö–ê: –í–†–ê–¢–ê–†–¨ ===
TabGoalkeeper:CreateToggle({
   Name = "ü•Ö –ê–≤—Ç–æ-—Å–µ–π–≤ (–í—Ä–∞—Ç–∞—Ä—å)",
   CurrentValue = false,
   Callback = function(Value) 
       Settings.AutoSave = Value 
       if Value then
           Rayfield:Notify({
               Title = "ü•Ö –†–µ–∂–∏–º –≤—Ä–∞—Ç–∞—Ä—è –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω", 
               Content = "–í—Å—Ç–∞–Ω—å—Ç–µ –≤ –≤–æ—Ä–æ—Ç–∞! –í—Å–µ —É–¥–∞—Ä—ã –±—É–¥—É—Ç –æ—Ç–±–∏—Ç—ã!", 
               Duration = 4
           })
       else
           Rayfield:Notify({
               Title = "‚ùå –ê–≤—Ç–æ-—Å–µ–π–≤ –≤—ã–∫–ª—é—á–µ–Ω", 
               Content = "", 
               Duration = 2
           })
       end
   end,
})

TabGoalkeeper:CreateToggle({
   Name = "üèÉ –ê–≤—Ç–æ–¥–≤–∏–∂–µ–Ω–∏–µ –∫ –º—è—á—É",
   CurrentValue = true,
   Callback = function(Value) 
       Settings.AutoMove = Value 
   end,
})

TabGoalkeeper:CreateSlider({
   Name = "üìè –†–∞–¥–∏—É—Å –∑–∞—â–∏—Ç—ã –≤–æ—Ä–æ—Ç",
   Range = {10, 40},
   Increment = 2,
   CurrentValue = 20,
   Callback = function(Value) Settings.SaveDistance = Value end,
})

TabGoalkeeper:CreateSlider({
   Name = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å –æ—Ç–±–∏–≤–∞–Ω–∏—è",
   Range = {60, 250},
   Increment = 10,
   CurrentValue = 120,
   Callback = function(Value) Settings.SaveSpeed = Value end,
})

TabGoalkeeper:CreateToggle({
   Name = "üëÅÔ∏è –ü–æ–∫–∞–∑–∞—Ç—å –∑–æ–Ω—É –∑–∞—â–∏—Ç—ã",
   CurrentValue = false,
   Callback = function(Value) 
       if Value then
           if not goalkeeperZone then
               goalkeeperZone = Instance.new("Part")
               goalkeeperZone.Name = "GoalkeeperZone"
               goalkeeperZone.Anchored = true
               goalkeeperZone.CanCollide = false
               goalkeeperZone.Shape = Enum.PartType.Ball
               goalkeeperZone.Material = Enum.Material.Neon
               goalkeeperZone.Color = Color3.fromRGB(255, 50, 50)
               goalkeeperZone.Transparency = 0.75
               goalkeeperZone.Size = Vector3.new(Settings.SaveDistance * 2, Settings.SaveDistance * 2, Settings.SaveDistance * 2)
               goalkeeperZone.Parent = workspace
               
               local highlight = Instance.new("SelectionBox")
               highlight.LineThickness = 0.08
               highlight.Color3 = Color3.fromRGB(255, 0, 0)
               highlight.Adornee = goalkeeperZone
               highlight.Parent = goalkeeperZone
           end
       else
           if goalkeeperZone then 
               goalkeeperZone:Destroy() 
               goalkeeperZone = nil
           end
       end
   end,
})

TabGoalkeeper:CreateSection("‚ÑπÔ∏è –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è")

TabGoalkeeper:CreateParagraph({
   Title = "–ö–∞–∫ —Ä–∞–±–æ—Ç–∞–µ—Ç –∞–≤—Ç–æ-—Å–µ–π–≤?",
   Content = "–í—Å—Ç–∞–Ω—å—Ç–µ –≤ –≤–æ—Ä–æ—Ç–∞ (GoalieCenter). –ö–æ–≥–¥–∞ –º—è—á –ª–µ—Ç–∏—Ç –í –°–¢–û–†–û–ù–£ –≤–∞—à–∏—Ö –≤–æ—Ä–æ—Ç –Ω–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏, —Å–∫—Ä–∏–ø—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –æ—Ç–±–∏–≤–∞–µ—Ç –µ–≥–æ. –†–∞–±–æ—Ç–∞–µ—Ç –¢–û–õ–¨–ö–û –≤ –∏–≥—Ä–µ, –Ω–µ –≤ –ª–æ–±–±–∏!"
})

TabGoalkeeper:CreateButton({
   Name = "üéØ –ù–∞–π—Ç–∏ –≤–æ—Ä–æ—Ç–∞ (–¥–µ–±–∞–≥)",
   Callback = function()
       local goal = getMyGoal()
       if goal then
           local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - goal.Position).Magnitude
           Rayfield:Notify({
               Title = "‚úÖ –í–∞—à–∏ –≤–æ—Ä–æ—Ç–∞ –Ω–∞–π–¥–µ–Ω—ã", 
               Content = "–î–∏—Å—Ç–∞–Ω—Ü–∏—è –¥–æ –≤–æ—Ä–æ—Ç: " .. math.floor(dist) .. "–º", 
               Duration = 5
           })
       else
           Rayfield:Notify({
               Title = "‚ö†Ô∏è –í–æ—Ä–æ—Ç–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã", 
               Content = "–ó–∞–π–¥–∏—Ç–µ –≤ –º–∞—Ç—á!", 
               Duration = 3
           })
       end
   end,
})

TabGoalkeeper:CreateButton({
   Name = "üìç –¢–µ–ª–µ–ø–æ—Ä—Ç –≤ –≤–æ—Ä–æ—Ç–∞",
   Callback = function()
       local goal = getMyGoal()
       local char = game.Players.LocalPlayer.Character
       if goal and char and char:FindFirstChild("HumanoidRootPart") then
           char.HumanoidRootPart.CFrame = goal.CFrame * CFrame.new(0, 3, 5)
