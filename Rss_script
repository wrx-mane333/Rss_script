    Name = "üìè –†–∞–¥–∏—É—Å —Ö–∏—Ç–±–æ–∫—Å–∞",
    Range = {3, 30},
    Increment = 1,
    CurrentValue = Settings.HitboxRadius,
    Callback = function(v)
        Settings.HitboxRadius = v
        if hitboxPart then 
            hitboxPart.Size = Vector3.new(v * 2, v * 2, v * 2) 
        end
    end
})

