local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "RSS | Hitbox Magnet v2.0",
    LoadingTitle = "–ó–∞–≥—Ä—É–∑–∫–∞ Hitbox Magnet...",
    LoadingSubtitle = "Fixed Auto GK + Improved Visual",
    ConfigurationSaving = { Enabled = false }
})

local Tab = Window:CreateTab("üß≤ –•–∏—Ç–±–æ–∫—Å", 4483362458)
local TabGK = Window:CreateTab("ü•Ö –í—Ä–∞—Ç–∞—Ä—å", 4483362458)

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local Settings = {
    HitboxEnabled = false,
    VisualizerEnabled = true,
    VisualizerTransparency = 0.8,  -- –ù–æ–≤–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞ –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç–∏
    HitboxRadius = 8,
    HitboxStrength = 80,
    HitboxUseBodyVelocity = true,
    PerBallCooldown = 0.35,
    GKMode = true,
    AutoGK = true,  -- –ê–≤—Ç–æ –≤—Ä–∞—Ç–∞—Ä—å
    GK_SaveSpeed = 140,
    GK_MinBallSpeed = 15,
    GK_DotThreshold = 0.7,
    GK_MaxDistanceToAttempt = 25,
    GK_ReactionTime = 0.1
}

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local workspace = game:GetService("Workspace")
local Debris = game:GetService("Debris")

local hitboxPart = nil
local activeBV = {}
local perBallLast = {}
local connection = nil
local lastGKCheck = 0

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –º—è—á–∞
local function isBall(part)
    if not part then return false end
    local name = part.Name:lower()
    return name:find("ball") or name:find("soccer") or name:find("balloon") or 
           name:find("puck") or (part.Size.Magnitude < 5 and part:GetAttribute("Ball") == true)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –≤–æ—Ä–æ—Ç
local function findMyGoal()
    local char = LocalPlayer.Character
    if not char then return nil end
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return nil end
    
    -- –ò—â–µ–º GoalCenter, GoalieCenter, GoalPost –∏–ª–∏ –ª—é–±—É—é —á–∞—Å—Ç—å —Å–≤—è–∑–∞–Ω–Ω—É—é —Å –≤–æ—Ä–æ—Ç–∞–º–∏
    local goalParts = {}
    
    -- –°–Ω–∞—á–∞–ª–∞ –∏—â–µ–º –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            if name:find("goal") or name:find("post") or name:find("net") then
                table.insert(goalParts, obj)
            end
        end
    end
    
    -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ —á–∞—Å—Ç–∏ –≤–æ—Ä–æ—Ç, –≤—ã–±–∏—Ä–∞–µ–º –±–ª–∏–∂–∞–π—à—É—é
    if #goalParts > 0 then
        local closest = nil
        local minDist = math.huge
        
        for _, goal in pairs(goalParts) do
            local dist = (hrp.Position - goal.Position).Magnitude
            if dist < minDist then
                minDist = dist
                closest = goal
            end
        end
        return closest
    end
    
    -- –ï—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏, –∏—â–µ–º –º–æ–¥–µ–ª–∏ Goal
    for _, model in pairs(workspace:GetChildren()) do
        if model:IsA("Model") and (model.Name:find("Goal") or model.Name:find("Goalie")) then
            local primary = model:FindFirstChild("PrimaryPart") or model:FindFirstChildWhichIsA("BasePart")
            if primary then
                return primary
            end
        end
    end
    
    return nil
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ —É–≥—Ä–æ–∑—ã –≤–æ—Ä–æ—Ç–∞–º
local function isBallThreateningGoal(ball, ballVel)
    if not Settings.GKMode or not Settings.AutoGK then return false end
    
    local goal = findMyGoal()
    if not goal then return false end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –º—è—á–∞
    local speed = ballVel.Magnitude
    if speed < Settings.GK_MinBallSpeed then return false end
    
    -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∫ –≤–æ—Ä–æ—Ç–∞–º
    local toGoal = (goal.Position - ball.Position)
    if toGoal.Magnitude > 50 then return false end  -- –°–ª–∏—à–∫–æ–º –¥–∞–ª–µ–∫–æ
    
    -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –¥–≤–∏–∂–µ–Ω–∏—è –º—è—á–∞
    local ballDir = ballVel.Unit
    local goalDir = toGoal.Unit
    
    -- –£–≥–æ–ª –º–µ–∂–¥—É –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ–º –º—è—á–∞ –∏ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ–º –∫ –≤–æ—Ä–æ—Ç–∞–º
    local dot = ballDir:Dot(goalDir)
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –ª–µ—Ç–∏—Ç –ª–∏ –º—è—á –≤ —Å—Ç–æ—Ä–æ–Ω—É –≤–æ—Ä–æ—Ç
    return dot > Settings.GK_DotThreshold
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —Å–µ–π–≤–∞ –≤—Ä–∞—Ç–∞—Ä—è
local function performGKSave(ball)
    if not Settings.GKMode or not Settings.AutoGK then return end
    
    local goal = findMyGoal()
    if not goal then return end
    
    local char = LocalPlayer.Character
    if not char then return end
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ –≤–æ—Ä–æ—Ç
    local distToGoal = (hrp.Position - goal.Position).Magnitude
    if distToGoal > Settings.GK_MaxDistanceToAttempt then return end
    
    -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –¥–ª—è —Å–µ–π–≤–∞ (–æ—Ç –≤–æ—Ä–æ—Ç)
    local saveDir = (ball.Position - goal.Position).Unit
    saveDir = Vector3.new(saveDir.X, 0.35, saveDir.Z).Unit
    
    -- –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–∏–ª—É
    pcall(function()
        ball.AssemblyLinearVelocity = saveDir * Settings.GK_SaveSpeed
    end)
    
    -- –í–∏–∑—É–∞–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
    Rayfield:Notify({
        Title = "ü•Ö –ê–≤—Ç–æ-—Å–µ–π–≤!",
        Content = "–ú—è—á –æ—Ç–±–∏—Ç!",
        Duration = 1
    })
end

local function createHitbox()
    if hitboxPart then 
        hitboxPart:Destroy() 
        hitboxPart = nil
    end
    
    hitboxPart = Instance.new("Part")
    hitboxPart.Name = "RSS_Hitbox"
    hitboxPart.Anchored = true
    hitboxPart.CanCollide = false
    hitboxPart.Transparency = Settings.VisualizerEnabled and Settings.VisualizerTransparency or 1
    hitboxPart.Color = Color3.fromRGB(255, 100, 100)  -- –°–≤–µ—Ç–ª–æ-–∫—Ä–∞—Å–Ω—ã–π —Ü–≤–µ—Ç
    hitboxPart.Size = Vector3.new(Settings.HitboxRadius * 2, Settings.HitboxRadius * 2, Settings.HitboxRadius * 2)
    hitboxPart.Shape = Enum.PartType.Ball
    hitboxPart.Material = Enum.Material.Neon
    hitboxPart.Parent = workspace
    
    -- –î–æ–±–∞–≤–ª—è–µ–º —Å–≤–µ—á–µ–Ω–∏–µ
    local glow = Instance.new("SurfaceGui", hitboxPart)
    glow.Face = Enum.NormalId.Front
    glow.AlwaysOnTop = true
    
    local frame = Instance.new("Frame", glow)
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
    frame.BackgroundTransparency = 0.7
    frame.BorderSizePixel = 0
end

local function destroyHitbox()
    if hitboxPart then
        hitboxPart:Destroy()
        hitboxPart = nil
    end
end

local function affectBall(ball)
    if not ball or not ball:IsA("BasePart") then return end
    
    local now = tick()
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫—É–ª–¥–∞—É–Ω
    if perBallLast[ball] and (now - perBallLast[ball]) < Settings.PerBallCooldown then
        return
    end
    
    perBallLast[ball] = now
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∞–≤—Ç–æ-—Å–µ–π–≤ –≤—Ä–∞—Ç–∞—Ä—è
    if Settings.GKMode and Settings.AutoGK then
        local ballVel = ball.AssemblyLinearVelocity
        if isBallThreateningGoal(ball, ballVel) then
            performGKSave(ball)
            return
        end
    end
    
    -- –û–±—ã—á–Ω–∞—è —Ö–∏—Ç–±–æ–∫—Å –ª–æ–≥–∏–∫–∞
    local char = LocalPlayer.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local targetPos = hrp.Position + Vector3.new(0, 1.5, 0)
    local dir = (targetPos - ball.Position)
    
    if dir.Magnitude > 0.1 then
        local dirUnit = dir.Unit
        
        if Settings.HitboxUseBodyVelocity then
            -- –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–π BodyVelocity
            local oldBV = ball:FindFirstChild("RSS_HitBV")
            if oldBV then oldBV:Destroy() end
            
            -- –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–π
            local bv = Instance.new("BodyVelocity")
            bv.Name = "RSS_HitBV"
            bv.MaxForce = Vector3.new(400000, 400000, 400000)
            bv.Velocity = dirUnit * Settings.HitboxStrength
            bv.Parent = ball
            
            activeBV[ball] = bv
            
            -- –ê–≤—Ç–æ-–æ—á–∏—Å—Ç–∫–∞ —á–µ—Ä–µ–∑ Debris
            Debris:AddItem(bv, 0.3)
            
            -- –û—á–∏—â–∞–µ–º –∏–∑ —Ç–∞–±–ª–∏—Ü—ã
            task.delay(0.3, function()
                activeBV[ball] = nil
            end)
        else
            -- –ü—Ä—è–º–æ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏
            pcall(function()
                ball.AssemblyLinearVelocity = dirUnit * Settings.HitboxStrength
            end)
        end
    end
end

local function checkBallsInRadius()
    if not Settings.HitboxEnabled or not hitboxPart then return end
    
    local position = hitboxPart.Position
    local radius = Settings.HitboxRadius
    
    -- –ò—â–µ–º –≤—Å–µ –º—è—á–∏ –≤ —Ä–∞–¥–∏—É—Å–µ
    for _, ball in pairs(workspace:GetPartsInPart(hitboxPart)) do
        if ball and ball:IsA("BasePart") and isBall(ball) then
            local distance = (ball.Position - position).Magnitude
            if distance <= radius then
                affectBall(ball)
            end
        end
    end
    
    -- –¢–∞–∫–∂–µ –ø—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ —à–∞—Ä—ã –≤ –∏–≥—Ä–µ –¥–ª—è GK
    if Settings.GKMode and Settings.AutoGK then
        local now = tick()
        if now - lastGKCheck >= Settings.GK_ReactionTime then
            lastGKCheck = now
            
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") and isBall(obj) then
                    local ballVel = obj.AssemblyLinearVelocity
                    if isBallThreateningGoal(obj, ballVel) then
                        performGKSave(obj)
                        break  -- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ–¥–∏–Ω –º—è—á –∑–∞ —Ä–∞–∑
                    end
                end
            end
        end
    end
end

local function startUpdater()
    if connection then 
        connection:Disconnect() 
        connection = nil
    end
    
    createHitbox()
    
    connection = RunService.Heartbeat:Connect(function()
        if not Settings.HitboxEnabled then 
            if hitboxPart then
                hitboxPart.Transparency = 1
            end
            return 
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –ø–æ–∑–∏—Ü–∏—é —Ö–∏—Ç–±–æ–∫—Å–∞
        local char = LocalPlayer.Character
        if char then
            local hrp = char:FindFirstChild("HumanoidRootPart")
            if hrp and hitboxPart then
                hitboxPart.Position = hrp.Position
                
                -- –û–±–Ω–æ–≤–ª—è–µ–º –≤–∏–¥–∏–º–æ—Å—Ç—å
                hitboxPart.Transparency = Settings.VisualizerEnabled and Settings.VisualizerTransparency or 1
                hitboxPart.Size = Vector3.new(Settings.HitboxRadius * 2, Settings.HitboxRadius * 2, Settings.HitboxRadius * 2)
                
                -- –ú–µ–Ω—è–µ–º —Ü–≤–µ—Ç –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä–µ–∂–∏–º–∞
                if Settings.GKMode and Settings.AutoGK then
                    hitboxPart.Color = Color3.fromRGB(100, 150, 255)  -- –°–∏–Ω–∏–π –¥–ª—è GK
                else
                    hitboxPart.Color = Color3.fromRGB(255, 100, 100)  -- –ö—Ä–∞—Å–Ω—ã–π –¥–ª—è –æ–±—ã—á–Ω–æ–≥–æ
                end
            end
        end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –º—è—á–∏
        checkBallsInRadius()
    end)
end

local function stopUpdater()
    if connection then 
        connection:Disconnect() 
        connection = nil
    end
    
    destroyHitbox()
    
    -- –û—á–∏—â–∞–µ–º –≤—Å–µ BodyVelocity
    for _, bv in pairs(activeBV) do
        if bv and bv.Parent then
            pcall(function() bv:Destroy() end)
        end
    end
    activeBV = {}
    perBallLast = {}
end

-- UI Elements
Tab:CreateToggle({
    Name = "üß≤ –í–∫–ª—é—á–∏—Ç—å –•–∏—Ç–±–æ–∫—Å",
    CurrentValue = false,
    Callback = function(val)
        Settings.HitboxEnabled = val
        if val then
            startUpdater()
            Rayfield:Notify({
                Title = "‚úÖ –•–∏—Ç–±–æ–∫—Å –≤–∫–ª—é—á–µ–Ω",
                Content = "–†–∞–¥–∏—É—Å: " .. Settings.HitboxRadius,
                Duration = 2
            })
        else
            stopUpdater()
            Rayfield:Notify({
                Title = "‚ùå –•–∏—Ç–±–æ–∫—Å –≤—ã–∫–ª—é—á–µ–Ω",
                Content = "",
                Duration = 1
            })
        end
    end
})

Tab:CreateToggle({
    Name = "üëÅÔ∏è –ü–æ–∫–∞–∑–∞—Ç—å –•–∏—Ç–±–æ–∫—Å",
    CurrentValue = true,
    Callback = function(val)
        Settings.VisualizerEnabled = val
        if hitboxPart then
            hitboxPart.Transparency = val and Settings.VisualizerTransparency or 1
        end
        Rayfield:Notify({
            Title = "üëÅÔ∏è –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è: " .. (val and "–í–ö–õ" or "–í–´–ö–õ"),
            Content = "",
            Duration = 1
        })
    end
})

Tab:CreateSlider({
    Name = "üéØ –ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å —Ö–∏—Ç–±–æ–∫—Å–∞",
    Range = {0.3, 0.9},
    Increment = 0.05,
    CurrentValue = 0.8,
    Callback = function(v)
        Settings.VisualizerTransparency = v
        if hitboxPart and Settings.VisualizerEnabled then
            hitboxPart.Transparency = v
        end
    end
})

