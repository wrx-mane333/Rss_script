TabGK:CreateButton({
    Name = "üîç –ù–∞–π—Ç–∏ –º–æ–∏ –≤–æ—Ä–æ—Ç–∞",
    Callback = function()
        local goal = findMyGoal()
        if goal then
            Rayfield:Notify({
                Title = "ü•Ö –í–æ—Ä–æ—Ç–∞ –Ω–∞–π–¥–µ–Ω—ã!",
                Content = "–ü–æ–∑–∏—Ü–∏—è: " .. math.floor(goal.Position.X) .. ", " .. 
                         math.floor(goal.Position.Y) .. ", " .. math.floor(goal.Position.Z),
                Duration = 3
            })
        else
            Rayfield:Notify({
                Title = "‚ùå –í–æ—Ä–æ—Ç–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã",
                Content = "–ü—Ä–æ–≤–µ—Ä—å—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –≤–æ—Ä–æ—Ç (–¥–æ–ª–∂–Ω–æ —Å–æ–¥–µ—Ä–∂–∞—Ç—å 'Goal')",
                Duration = 3
            })
        end
    end
})

-- Cleanup
game:BindToClose(function()
    stopUpdater()
end)

Rayfield:Notify({
    Title = "‚úÖ –°–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω",
    Content = "Hitbox Magnet v2.0 –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ!\n–ê–≤—Ç–æ-–≤—Ä–∞—Ç–∞—Ä—å: –í–ö–õ",
    Duration = 5
})

-- –î–µ–±–∞–≥ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
local function printDebugInfo()
    print("=== RSS Hitbox Debug ===")
    print("Hitbox Enabled:", Settings.HitboxEnabled)
    print("GK Mode:", Settings.GKMode)
    print("Auto GK:", Settings.AutoGK)
    print("Hitbox Radius:", Settings.HitboxRadius)
    print("Visualizer Transparency:", Settings.VisualizerTransparency)
    print("=====================")
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –¥–µ–±–∞–≥
task.wait(1)
printDebugInfo()
